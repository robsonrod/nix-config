# git alias
alias g='git'
alias nb='git checkout -b "$USER/$(date +%Y%m%d)"'
alias clone='git clone'
alias adda='git add . --all'
alias addu='git add -u'
alias status='git status'
alias commit='git commit -v -m'
alias chechout='git checkout'
alias chechoutb='git checkout -b'
alias gdiff='git diff'
alias pull='git pull'
alias glog="git log --graph --pretty=format:'%C(bold)%h%Creset%C(magenta)%d%Creset %s %C(yellow)<%an> %C(cyan)(%cr)%Creset' --abbrev-commit --date=relative"
alias groot='git rev-parse --show-toplevel'
alias fecth='git fetch --all --prune --verbose'
alias reset='git reset HEAD'
alias gpp='git pull && git push'
alias push='git push origin'
alias gclear='git clean -xfd'
alias dbranch='git branch -Dd'
alias gmain='git checkout main'
alias stashl='git stash list'
alias stashp='git stash pop'
